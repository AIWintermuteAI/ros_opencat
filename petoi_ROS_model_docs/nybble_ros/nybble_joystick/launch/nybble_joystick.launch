<launch>

    <!-- Joystick node -->
    <node pkg="joy" type="joy_node" name="JOYSTICK">
        <param name="joy_node/dev" value="/dev/input/js0"/>
    </node>

  
         <!-- NYBBLE Joystick node -->
  <node pkg="nybble_joystick" type="nybble_joystick.py"
        name="NYBBLE_JOYSTICK" output="screen"/>

</launch>
