<launch>

    <!-- Joystick node -->
    <node pkg="joy" type="joy_node" name="JOYSTICK">
        <param name="joy_node/dev" value="/dev/input/js0"/>
    </node>

  
         <!-- BITTLE Joystick node -->
  <node pkg="bittle_joystick" type="bittle_joystick.py"
        name="BITTLE_JOYSTICK" output="screen"/>

</launch>
